<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
		android:theme="@style/AppTheme">
        <activity android:name=".ADBActivity" android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
		</activity>
        <activity
			android:name=".FastbootActivity"
            android:exported="false"
			android:label="@string/fastboot">
		</activity>
		<activity 
			android:name=".AppListActivity"
            android:exported="false"
			android:label="@string/app_list">
		</activity>
		<activity 
			android:name=".ADBFileManagerActivity"
            android:exported="false"
			android:label="@string/file_manager">
		</activity>
		<activity 
			android:name=".FileManagerActivity"
            android:exported="false"
			android:label="@string/file_manager">
		</activity>
		<activity 
			android:name=".AppManagerActivity"
            android:exported="false"
			android:label="@string/app_manager">
		</activity>
		<activity 
			android:name=".PermissionsActivity"
            android:exported="false"
			android:label="@string/app_permissions">
		</activity>
    </application>
</manifest>
